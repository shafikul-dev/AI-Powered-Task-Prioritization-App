{
  "name": "ai-powered-task-prioritization-app",
  "version": "1.0.0",
  "description": "A smart to-do list application that uses AI to prioritize and categorize tasks",
  "main": "server.js",
  "scripts": {
    "dev": "concurrently \"npm:server\" \"npm:client\"",
    "start": "node server.js",
    "server": "nodemon server.js",
    "client": "cd client && npm start",
    "client-build": "cd client && npm run build",
    "setup": "node setup.js",
    "demo": "node demo-test.js",
    "build": "npm run client-build",
    "heroku-postbuild": "npm run build"
  },
  "keywords": [
    "ai",
    "task-management",
    "prioritization",
    "todo",
    "productivity",
    "react"
  ],
  "author": "AI Task Prioritization App",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2"
  },
  "devDependencies": {
    "concurrently": "^9.2.1",
    "nodemon": "^3.0.1"
  }
}
